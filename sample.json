{
    "mainDispatcher":"main",
    "dispatchers":[
        {
            "name":"main",
            "dispatch":[
                {
                    "pattern":".*login:",
                    "textToSend":"$SG_LOGIN$",
                    "nextDispatcher":"main",
                    "timeout":10
                },
                {
                    "pattern":"[P|p]assword:",
                    "textToSend":"$SG_PASSWORD$",
                    "nextDispatcher":"main",
                    "timeout":10
                },
                {
                    "pattern":"root.*]#",
                    "textToSend":"ifconfig",
                    "nextDispatcher":"ifconfig",
                    "timeout":10
                }
            ]
        },
        {
            "name":"ifconfig",
            "dispatch":[
                { 
                    "pattern":"root.*]#",
                    "textToSend":"cat /var/log/messages",
                    "nextDispatcher":"catLog",
                    "timeout":5
                }
            ]
        },
        {
            "name":"catLog",
            "dispatch":[
                {
                    "pattern":"root.*]#",
                    "textToSend":"exit",
                    "nextDispatcher":"exitFailure",
                    "timeout":5
                }
            ]
        }
    ]
}
